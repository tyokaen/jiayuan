<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jiayuan.bijin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.user_main" />
        <activity android:name=".Activity.signup" />
        <activity android:name=".Activity.signup2" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true" />
        <activity android:name=".Activity.Image_Choice" />
        <activity android:name=".Activity.function_choice" />
        <activity
            android:name=".Activity.entrance"
            android:label="@string/title_activity_splash_screen"
            android:theme="@style/AppTheme" />
        <activity android:name=".Fragment.bijin_detail" />
        <activity android:name=".Fragment.bijin_best_detail" />
        <activity
            android:name=".Activity.Pre_recommend"
            android:label="@string/title_activity_pre_recommend"
            android:theme="@style/AppTheme" />
        <activity android:name=".Activity.recommend"></activity>
    </application>

</manifest>